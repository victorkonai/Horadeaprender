<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.horadeaprender">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".iu.DesempenhoActivity"
            android:label="Desempenho"/>
        <activity
            android:name=".iu.SimuladoActivity"
            android:label="Simulado"/>
        <activity
            android:name=".iu.ConfiguracoesActivity"
            android:label="Configurações" />
        <activity
            android:name=".iu.SobreActivity"
            android:label="Sobre" />
        <activity
            android:name=".iu.EnviarQuestaoActivity"
            android:label="Enviar questão" />
        <activity
            android:name=".iu.VerificarDesempenhoActivity"
            android:label="Verificar desempenho" />
        <activity
            android:name=".iu.OpcoesSimuladoActivity"
            android:label="Opções de simulado" />
        <activity
            android:name=".iu.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>